#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules/nanoid/bin/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules/nanoid/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules/nanoid/bin/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules/nanoid/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/nanoid@3.3.11/node_modules:/mnt/c/Users/강민주/Desktop/민주/UMC/WEB_STUDY/주민_강민주/2주차_미션/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../bin/nanoid.cjs" "$@"
fi
